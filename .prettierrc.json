{
    "bracketSpacing": false,
    "printWidth": 120,
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "endOfLine": "lf",
    "tabWidth": 4,
    "useTabs": false,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "jsxBracketSameLine": true,
    "arrowParens": "avoid",
    "requirePragma": false,
    "insertPragma": false,
    "proseWrap": "preserve"
}
